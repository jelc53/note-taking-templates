\ProvidesPackage{math}

% PACKAGES

% add [fleqn] to left justify
\usepackage[margin=1in]{geometry}

% typographic corrections for English
\usepackage[english]{babel}

% adds tools
\usepackage{mathtools}

% includes amsmath, fixes a few things, adds a few environments
% https://en.wikibooks.org/wiki/LaTeX/Mathematics
\usepackage{amssymb}

% required for math symbols
\usepackage{amsthm}

% provides blackboard fonts for indicator function, \mathbbm{1}
\usepackage{bbm}

% contains a lot of math shorthands
\usepackage{physics}

% required for proof environment
\usepackage[outputdir=build]{minted}

% code syntax highlighting
% https://www.overleaf.com/learn/latex/Code_Highlighting_with_minted
\usepackage{hyperref}

% allows hyperlinks, i.e. \href{url}{name}
\usepackage{enumitem}

% specify alpha/num for enums
\usepackage{multirow}

% for tables
\usepackage{fancyvrb}

% Verbatim with frame
\usepackage{tcolorbox}

% colored boxes around text
\usepackage[pdf]{graphviz}

% for graphs/drawings
\usepackage{tikz}

% provides things like indicator functions with \mathbb{1}
\usepackage{unicode-math}

% provides accents, including \undertilde
\usepackage{accents}

% FORMATTING
\setlength{\parindent}{0em}
\setlength{\parskip}{1em}

% COMMANDS
\newcommand{\cmd}{\newcommand}
\newcommand{\re}{\renewcommand}
